{
  "name": "ncfg-cms",
  "private": true,
  "version": "0.1.0",
  "description": "NCFG Strapi CMS",
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "deploy": "strapi deploy",
    "copy:schemas": "node -e \"const fs=require('fs');const path=require('path');const copyDir=(src,dest)=>{if(!fs.existsSync(src))return;fs.mkdirSync(dest,{recursive:true});fs.readdirSync(src,{withFileTypes:true}).forEach(f=>{const srcPath=path.join(src,f.name);const destPath=path.join(dest,f.name);f.isDirectory()?copyDir(srcPath,destPath):f.name.endsWith('.json')&&fs.copyFileSync(srcPath,destPath)})};['news-article','tag','service','service-category','person'].forEach(api=>copyDir('src/api/'+api+'/content-types','dist/src/api/'+api+'/content-types'));copyDir('src/components','dist/src/components')\""
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "typescript": "^5.7.0"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "^5.34.0",
    "@strapi/provider-upload-aws-s3": "^5.34.0",
    "@strapi/plugin-users-permissions": "^5.34.0",
    "@strapi/strapi": "^5.34.0",
    "better-sqlite3": "^11.8.0",
    "pg": "^8.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "styled-components": "^6.1.14"
  },
  "engines": {
    "node": ">=20.0.0 <=22.x.x",
    "npm": ">=10.0.0"
  },
  "strapi": {
    "uuid": "ncfg-cms-001"
  },
  "license": "MIT"
}
